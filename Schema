CREATE TABLE DOG(
DOGID INT NOT NULL,
BREED VARCHAR(50) NULL,
NAME VARCHAR(25) NULL,
DOB DATE NOT NULL,
VETID INT NULL,
USER_CREATED DATE NULL,
USER_UPDATED DATE NULL,

CONSTRAINT PK_DOG PRIMARY KEY (DOGID),
CONSTRAINT FK_VET FOREIGN KEY (VETID) REFRENCES VET (VETID)
);

CREATE TABLE OWNER (
OID INT NOT NULL,
FNAME VARCHAR(20),
LNAME VARCHAR(20),
DOGID INT NOT NULL,
USER_CREATED DATE NULL,
USER_UPDATED DATE NULL,

CONSTRAINT PK_OWNER PRIMARY KEY (OID),
CONSTRAINT FK_DOGID FOREIGN KEY 
(DOGID) REFERENCES DOG (DOGID)
);

CREATE TABLE BOARDING (
BID INT NOT NULL,
OID INT NOT NULL, 
LAST_STAY DATE NULL,
USER_CREATED DATE NULL,
USER_UPDATED DATE NULL,

CONSTRAINT PK_BOARDING PRIMARY KEY (REC_ID),
CONSTRAINT FK_OID FOREIGN KEY
(OID) REFERENCES OWNER (OID)
);

CREATE TABLE PAYMENT (
PAYID INT NOT NULL,
OID INT NOT NULL,
CUSTNAME VARCHAR(50) NOT NULL,
CCNUM CHAR(16) NULL, 
ADDRESS VARCHAR(50) NOT NULL,
ZIPCODE CHAR(5) NOT NULL,
EXPDATE DATE NOT NULL,
TOTAL INT NOT NULL,
USER_CREATED DATE NULL,
USER_UPDATED DATE NULL,

CONSTRAINT PK_PAYMENT PRIMARY KEY (PAYID),
CONSTRAINT FK_OWNER FOREIGN KEY (OID) REFERENCES OWNER (OID)
);

CREATE TABLE ROOM (
RMID INT NOT NULL,
DAYS VARCHAR(10) NOT NULL,
VETID INT NULL,
USER_CREATED DATE NULL,
USER_UPDATED DATE NULL,

CONSTRAINT PK_ROOM PRIMARY KEY (RMID),
CONSTRAINT FK_VET FOREIGN KEY (VETID) REFERENCES VET (VETID)
);

CREATE TABLE VET (
VETID INT NOT NULL,
Name VARCHAR(50) NOT NULL,
PHONENUM CHAR(10) NOT NULL, 
HIREDATE DATE NOT NULL,
RMID INT NULL,
USER_CREATED DATE NULL,
USER_UPDATED DATE NULL,

CONSTRAINT PK_VET PRIMARY KEY (VETID),
CONSTRAINT FK_ROOM FOREIGN KEY (RMID) REFERENCES ROOM (RMID)
);














